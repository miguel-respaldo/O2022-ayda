def ordburbuja(array):
    num = len(array)

    for i in range(num-1):       
        for j in range(num-1-i): 
            if array[j] > array[j+1]:
                array[j], array[j+1] = array[j+1], array[j]

items = [10, 5, 9, 7, 11]
ordburbuja(items)

print(items)
