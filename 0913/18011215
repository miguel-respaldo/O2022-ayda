def fibonacci(num):
    if (num == 1):
        return 1
    elif (num == 0):
        return 0
    else:
        return (fibonacci(num - 2) + fibonacci(num - 1))


def main():
    numero = eval(input("Escribe un número: "))
    print("La posición", numero, "de fibonacci es", fibonacci(numero))


if __name__ == "__main__":
    main()
